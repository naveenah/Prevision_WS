# Django Configuration
# Generate a new SECRET_KEY by running: python -c "from django.core.management.utils import get_random_secret_key; print(get_random_secret_key())"
SECRET_KEY=your-secret-key-here-generate-new-one
DEBUG=True
ALLOWED_HOSTS=localhost,127.0.0.1

# Database Configuration (PostgreSQL)
# Option 1: Use DATABASE_URL (not currently used but available)
# DATABASE_URL=postgresql://your-db-user:your-db-password@your-host.neon.tech/your-database?sslmode=require&channel_binding=require

# Option 2: Use individual settings (currently active)
DB_NAME=your-database-name
DB_USER=your-db-user
DB_PASSWORD=your-db-password
DB_HOST=your-host.neon.tech
DB_PORT=5432
DB_SSLMODE=require
DB_CHANNEL_BINDING=require

# CORS Configuration
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# External Services
GOOGLE_API_KEY=your-google-api-key
GS_BUCKET_NAME=your-gcs-bucket-name
GS_PROJECT_ID=your-gcp-project-id
GS_CREDENTIALS_PATH=path/to/service-account.json

# Payment Services (to be added later)
# STRIPE_SECRET_KEY=your-stripe-secret-key
# STRIPE_PUBLISHABLE_KEY=your-stripe-publishable-key